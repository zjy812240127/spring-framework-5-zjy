<hik:user id="person" name="person" address="hikvision"></hik:user>
	<context:component-scan base-package="com.zjut"></context:component-scan>
	<context:annotation-config></context:annotation-config>

	<bean id="a" class="com.zjut.circleRefrence.A">
		<property name="b" ref="b"></property>
	</bean>

	<bean id="b" class="com.zjut.circleRefrence.B">
		<property name="a" ref="a"></property>
 	</bean>

