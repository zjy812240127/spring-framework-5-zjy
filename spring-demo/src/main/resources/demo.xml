	<bean id="myCustomizedPropertyEditorRegistrar" class="com.zjut.myConversionBean.MyCustomizedPropertyEditorRegistrar"></bean>
	<bean class="org.springframework.beans.factory.config.CustomEditorConfigurer">
		<property name="propertyEditorRegistrars">
			<list>
				<ref bean="myCustomizedPropertyEditorRegistrar"/>
			</list>
		</property>
	</bean>

	<bean id="myApplicationListener" class="com.zjut.listenner.MyApplicationListener"></bean>
	<bean id="myApplicationEventMulticaster" class="com.zjut.listenner.MyApplicationEventMulticaster"></bean>
	<bean id="personNamespaceHandler" class="com.zjut.PersonNamespaceHandler"></bean>
	<bean id="personPlaceholderBeanDefinitionParser" class="com.zjut.PersonPlaceholderBeanDefinitionParser"></bean>
	<bean id="reader" class="com.zjut.MyBeanDefinitionDocumentReader" ></bean>
	<bean id="myConversionBean" class="com.zjut.myConversionBean.MyConversionBean">
		<property name="innerBean" value="zhangsan_address"></property>
		<property name="name" value="lisi"></property>
	</bean>
